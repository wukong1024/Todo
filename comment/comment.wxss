.comment-box{padding:30rpx 30rpx 100rpx;margin-top: 20rpx;background: #fff;color: #333;}
.comment-title{
  font-size: 28rpx;
  margin-bottom: 30rpx;
}
.comment-title::before{
  content: '';
  display: inline-block;
  width: 2rpx;
  height: 26rpx;
  background: #3b5692;
  margin-right: 10rpx;
  vertical-align: middle;
}
.comment-list_item{display: flex;padding: 30rpx 0;position: relative}
.comment-list_item::after{
  content: '';
  height: 1px;
  position: absolute;
  bottom: 0;
  left: 80rpx;
  right: 0;
  background: #dcdcdc;
  transform: scaleY(0.5)
}
.comment-list_item:last-child:after{
  display: none;
}
.comment-list_item>image{
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}
.comment-content{flex: 1;margin-left: 18rpx;}
.comment-content_name{font-size: 28rpx;color: #3b5692;}
.comment-content_content{font-size: 32rpx;margin: 20rpx 0;}
.comment-content_info{font-size: 24rpx;color: #8f8f8f;}
.conmment-content_reply{margin-left: 20rpx;color: #3b5692}
.comment-del{float: right;color:#3b5692 }

.comment-like{float: right;}
.comment-like_icon{
  display: inline-block;
  width:28rpx;
  height:27rpx;
  margin-right: 12rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: contain;
}
.comment-like_icon.act{
  background: url(data:image/png;base64,) no-repeat;
  background-size: contain;
}

.comment-empty{padding:70rpx;text-align: center;}
.comment-empty>image{width: 117rpx;height: 117rpx;}
.comment-empty_info{font-size: 32rpx;color:#666;margin-top: 20rpx;}
.comment-nav{
  display: flex;
  height: 100rpx;
  border-top: 1rpx solid #ebebed;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background: #fff;
  box-sizing: border-box;
  align-items: center;
  font-size: 28rpx;
}
.comment-nav_input{flex: 1;}
.comment-nav input{
  background: #ebebed;
  border-radius: 40rpx;
  padding: 0 30rpx;
  width: 100%;
  box-sizing: border-box;
}
.comment-nav_arrow,.comment-nav_comment,.comment-nav_share,.comment-nav_collect{
  width: 40rpx;
  height: 40rpx;
  padding:30rpx;
  box-sizing: content-box;
}
.comment-nav_arrow{
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 23rpx 41rpx;
}
.comment-nav_comment{
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 36rpx 36rpx;
}
.comment-nav_share{
    background: url(data:image/png;base64,) no-repeat center;
  background-size: 32rpx 32rpx;
}
.comment-nav_share::after{
  border:none;
}
.comment-nav_collect{
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 40rpx 40rpx;
  position: fixed;
  top: 20rpx;
  right: 20rpx;
}
.comment-nav_collect.act{
   background: url(data:image/png;base64,) no-repeat center;
  background-size: 40rpx 40rpx;
  position: fixed;
  top: 20rpx;
  right: 20rpx;
}